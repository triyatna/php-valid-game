{
  "name": "triyatna/php-valid-game",
  "description": "PHP package for validating game user IDs via Codashop scraping and GoPay Games",
  "keywords": [
    "game",
    "validation",
    "codashop",
    "mobile-legends",
    "freefire",
    "game-id"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tri Yatna",
      "email": "triyatna.dev@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.0",
    "psr/log": "^2.0|^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0",
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Triyatna\\PhpValidGame\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Triyatna\\PhpValidGame\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Triyatna\\PhpValidGame\\Laravel\\ValidGameServiceProvider"
      ],
      "aliases": {
        "ValidGame": "Triyatna\\PhpValidGame\\Laravel\\Facades\\ValidGame"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
